{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfsltdev"
		},
		"optima_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'optima'"
		},
		"optimaStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'optimaStorage'"
		},
		"optimabatch_accessKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accessKey' of 'optimabatch'"
		},
		"optimabatch_properties_typeProperties_accountName": {
			"type": "string",
			"defaultValue": "optima"
		},
		"optimabatch_properties_typeProperties_batchUri": {
			"type": "string",
			"defaultValue": "https://optima.westeurope.batch.azure.com"
		},
		"optimabatch_properties_typeProperties_poolName": {
			"type": "string",
			"defaultValue": "Optima-engine"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfsltdev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"optima_connectionString": {
						"value": "[parameters('optima_connectionString')]"
					},
					"optimaStorage_connectionString": {
						"value": "[parameters('optimaStorage_connectionString')]"
					},
					"optimabatch_accessKey": {
						"value": "[parameters('optimabatch_accessKey')]"
					},
					"optimabatch_properties_typeProperties_accountName": {
						"value": "[parameters('optimabatch_properties_typeProperties_accountName')]"
					},
					"optimabatch_properties_typeProperties_batchUri": {
						"value": "[parameters('optimabatch_properties_typeProperties_batchUri')]"
					},
					"optimabatch_properties_typeProperties_poolName": {
						"value": "[parameters('optimabatch_properties_typeProperties_poolName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}