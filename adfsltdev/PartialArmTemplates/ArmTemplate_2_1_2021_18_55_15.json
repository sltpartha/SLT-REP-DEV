{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfsltdev"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/optimabatch')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBatch",
				"typeProperties": {
					"accountName": "optima",
					"accessKey": {
						"type": "SecureString",
						"value": "**********"
					},
					"batchUri": "https://optima.westeurope.batch.azure.com",
					"poolName": "Optima-engine",
					"linkedServiceName": {
						"referenceName": "optimaStorage",
						"type": "LinkedServiceReference"
					}
				}
			},
			"dependsOn": []
		}
	]
}