{
	"name": "PL_Plunet",
	"properties": {
		"activities": [
			{
				"name": "Plunet_SampleTables_Move",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "mitarbeiter"
					},
					{
						"name": "Destination",
						"value": "PLUNET.mitarbeiter"
					}
				],
				"typeProperties": {
					"source": {
						"type": "AzureMySqlSource"
					},
					"sink": {
						"type": "SqlDWSink",
						"tableOption": "autoCreate"
					},
					"enableStaging": true,
					"stagingSettings": {
						"linkedServiceName": {
							"referenceName": "AzureDataLakeStorage1",
							"type": "LinkedServiceReference"
						},
						"path": "stagingplunet"
					},
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "MitarbeiterID",
									"physicalType": "int"
								},
								"sink": {
									"name": "MitarbeiterID"
								}
							},
							{
								"source": {
									"name": "Vorname",
									"physicalType": "varchar"
								},
								"sink": {
									"name": "Vorname"
								}
							},
							{
								"source": {
									"name": "Nachname",
									"physicalType": "varchar"
								},
								"sink": {
									"name": "Nachname"
								}
							},
							{
								"source": {
									"name": "eMail",
									"physicalType": "longtext"
								},
								"sink": {
									"name": "eMail"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "SourceDataset_fhq",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DestinationDataset_fhq",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}