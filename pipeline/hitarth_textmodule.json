{
	"name": "hitarth_textmodule",
	"properties": {
		"activities": [
			{
				"name": "Copy_m2e",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "`angebot_textmodul`"
					},
					{
						"name": "Destination",
						"value": "slt_test.angebot_textmodul"
					}
				],
				"typeProperties": {
					"source": {
						"type": "MySqlSource",
						"query": "select\n   at.*,\n  SUBSTRING_INDEX(SUBSTRING_INDEX(AT.inhalt, ';', numbers.n), ';', -1) INHALT_NEW\nfrom\n  (select 1 n union all\n   select 2 union all select 3 union all\n   select 4 union all select 5 union all\n   select 6 union all select 7 union all\n   select 8 union all select 9) numbers INNER JOIN `angebot_textmodul` at\n  on CHAR_LENGTH(AT.inhalt)\n     -CHAR_LENGTH(REPLACE(AT.inhalt, ';', ''))>=numbers.n-1"
					},
					"sink": {
						"type": "SqlDWSink",
						"preCopyScript": "truncate table slt_test.angebot_textmodul",
						"allowPolyBase": true,
						"polyBaseSettings": {
							"rejectValue": 0,
							"rejectType": "value",
							"useTypeDefault": true
						},
						"tableOption": "autoCreate",
						"disableMetricsCollection": false
					},
					"enableStaging": true,
					"stagingSettings": {
						"linkedServiceName": {
							"referenceName": "AzureBlobStorage1",
							"type": "LinkedServiceReference"
						},
						"path": "stagingplunet"
					},
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "TextModulVerwendungID",
									"type": "Int32"
								},
								"sink": {
									"name": "TextModulVerwendungID",
									"type": "Int32"
								}
							},
							{
								"source": {
									"name": "IDTextModul",
									"type": "Int32"
								},
								"sink": {
									"name": "IDTextModul",
									"type": "Int32"
								}
							},
							{
								"source": {
									"name": "IDMain",
									"type": "Int32"
								},
								"sink": {
									"name": "IDMain",
									"type": "Int32"
								}
							},
							{
								"source": {
									"name": "Inhalt",
									"type": "String"
								},
								"sink": {
									"name": "Inhalt",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "Inhalt_Datum",
									"type": "DateTime"
								},
								"sink": {
									"name": "Inhalt_Datum",
									"type": "DateTime"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "SourceDataset_m2e",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DestinationDataset_m2e",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}