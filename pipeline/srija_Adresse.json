{
	"name": "srija_Adresse",
	"properties": {
		"activities": [
			{
				"name": "Copy_sxm",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "`job`"
					},
					{
						"name": "Destination",
						"value": "slt_test.Job"
					}
				],
				"typeProperties": {
					"source": {
						"type": "MySqlSource"
					},
					"sink": {
						"type": "SqlDWSink",
						"preCopyScript": "truncate table slt_test.Adresse",
						"tableOption": "autoCreate",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"validateDataConsistency": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "SD_Adresse",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DD_Adresse",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "srija"
		},
		"annotations": []
	}
}