					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureMySqlSource",
						"query": "select\n   at.*,\n  SUBSTRING_INDEX(SUBSTRING_INDEX(AT.subservice, ',', numbers.n), ',', -1) SubService_NEW\nfrom\n  (select 1 n union all\n   select 2 union all select 3 union all\n   select 4 union all select 5 union all\n   select 6 union all select 7 union all\n   select 8 union all select 9) numbers INNER JOIN slt_test.Account_subservice at\n  on CHAR_LENGTH(AT.subservice)\n     -CHAR_LENGTH(REPLACE(AT.subservice, ',', ''))>=numbers.n-1",
						"queryTimeout": "02:00:00"
					},
					"sink": {
						"type": "SqlDWSink",
						"preCopyScript": "truncate table CRM.Account_Subservice",
						"tableOption": "autoCreate",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "Accountid",
									"type": "String"
								},
								"sink": {
									"name": "Accountid",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "SubService",
									"type": "String"
								},
								"sink": {
