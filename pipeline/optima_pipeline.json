{
	"name": "optima_pipeline",
	"properties": {
		"activities": [
			{
				"name": "Optima",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "SqlDWSink",
						"preCopyScript": "truncate table optima.BnkRozKwotyView",
						"tableOption": "autoCreate",
						"disableMetricsCollection": false
					},
					"enableStaging": true,
					"stagingSettings": {
						"linkedServiceName": {
							"referenceName": "AzureBlobStorage1",
							"type": "LinkedServiceReference"
						}
					},
					"parallelCopies": 1,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "Optima_BnkRozKwotyView",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "Optima_Synapse_BnkRozKwotyView",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "Plunet_Old_Pipeline"
		},
		"annotations": []
	}
}