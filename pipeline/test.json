{
	"name": "test",
	"properties": {
		"description": "copy plunet data from azure my sql to synapse",
		"activities": [
			{
				"name": "Copy data",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "MySqlSource",
						"query": "select * from z_taba22"
					},
					"sink": {
						"type": "SqlDWSink",
						"tableOption": "autoCreate"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "ID",
									"type": "Int32"
								},
								"sink": {
									"name": "ID",
									"type": "Int32",
									"physicalType": "Int32"
								}
							},
							{
								"source": {
									"name": "Deutsch",
									"type": "String"
								},
								"sink": {
									"name": "Deutsch",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "English",
									"type": "String"
								},
								"sink": {
									"name": "English",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "ArtikelnummerTeil",
									"type": "String"
								},
								"sink": {
									"name": "ArtikelnummerTeil",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "Aktiv",
									"type": "Int32"
								},
								"sink": {
									"name": "Aktiv",
									"type": "Int32",
									"physicalType": "Int32"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": false,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "MySqlTable3",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AzureSynapseAnalyticsTable1",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}