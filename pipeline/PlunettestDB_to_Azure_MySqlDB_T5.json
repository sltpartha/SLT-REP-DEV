									"parameters": {
										"cw_tableName": "@item().source.tableName"
									}
								}
							],
							"outputs": [
								{
									"referenceName": "DestinationDataset_ucw",
									"type": "DatasetReference",
									"parameters": {
										"cw_tableName": "@item().destination.tableName"
