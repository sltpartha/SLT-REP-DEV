{
	"name": "custom_gender_data",
	"properties": {
		"activities": [
			{
				"name": "gender",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Destination",
						"value": "cntrawzonesltdev/BambooHR/FILE_BAMBOOHR"
					}
				],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "POST",
						"requestBody": {
							"value": "@{pipeline().parameters.body}",
							"type": "Expression"
						},
						"additionalHeaders": {
							"format": "XML"
						}
					},
					"sink": {
						"type": "JsonSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "JsonWriteSettings",
							"filePattern": "setOfObjects"
						}
					},
					"enableStaging": false,
					"validateDataConsistency": false
				},
				"inputs": [
					{
						"referenceName": "src_gender",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "custom_gender",
						"type": "DatasetReference"
					}
				]
			}
		],
		"parameters": {
			"body": {
				"type": "string",
				"defaultValue": {
					"title": "CustomGenderReport",
					"fields": [
						"firstName",
						"lastName",
						"employeeNumber",
						"customGenderCategory"
					]
				}
			}
		},
		"annotations": []
	}
}