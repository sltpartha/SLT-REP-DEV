{
	"name": "PL_CRM_TO_SYNAPSE_Account_subservice",
	"properties": {
		"description": "Moving CRM data to MS SQL DB",
		"activities": [
			{
				"name": "PL_CRM_TO_MSQL_Account",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "account"
					},
					{
						"name": "Destination",
						"value": "dbo.account"
					}
				],
				"typeProperties": {
					"source": {
						"type": "DynamicsCrmSource"
					},
					"sink": {
						"type": "AzureMySqlSink",
						"writeBatchSize": 10000,
						"writeBatchTimeout": "00:00:30"
					},
					"enableStaging": false,
					"validateDataConsistency": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "accountid",
									"type": "Guid"
								},
								"sink": {
									"name": "Accountid",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "erb_selectedsubservices",
									"type": "String"
								},
								"sink": {
									"name": "SubService",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "erb_selectedsubservicestext",
									"type": "String"
								},
								"sink": {
									"name": "SubService_Text",
									"type": "String"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "DS_REST_CRM_Account",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "CRM_MySQL_Subservice",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "CRM"
		},
		"annotations": []
	}
}