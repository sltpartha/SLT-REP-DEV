{
	"name": "SQL_to_Synapse",
	"properties": {
		"activities": [
			{
				"name": "ForEach_cox",
				"type": "ForEach",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@pipeline().parameters.cw_items",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "Copy_cox",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [
								{
									"name": "Source",
									"value": "sltsqldbdev.@{item().source.table}"
								},
								{
									"name": "Destination",
									"value": "sltsqldbdev.@{item().destination.table}"
								}
							],
							"typeProperties": {
								"source": {
									"type": "AzureSqlSource",
									"partitionOption": "None"
								},
								"sink": {
									"type": "SqlDWSink",
									"preCopyScript": {
										"value": "@concat('TRUNCATE TABLE sltsqldbdev.',item().destination.table)",
										"type": "Expression"
									},
									"tableOption": "autoCreate",
									"disableMetricsCollection": false
								},
								"enableStaging": false
							},
							"inputs": [
								{
									"referenceName": "DS_SQL_CRM",
									"type": "DatasetReference",
									"parameters": {
										"cw_table": "@item().destination.table"
									}
								}
							],
							"outputs": [
								{
									"referenceName": "DS_Synapse_CRM",
									"type": "DatasetReference",
									"parameters": {
										"cw_table": "@item().destination.table"
									}
								}
							]
						}
					]
				}
			}
		],
		"parameters": {
			"cw_items": {
				"type": "Array",
				"defaultValue": [
					{
						"source": {
							"table": "account"
						},
						"destination": {
							"table": "account"
						}
					},
					{
						"source": {
							"table": "contact"
						},
						"destination": {
							"table": "contact"
						}
					},
					{
						"source": {
							"table": "Currency"
						},
						"destination": {
							"table": "Currency"
						}
					},
					{
						"source": {
							"table": "customeraddress"
						},
						"destination": {
							"table": "customeraddress"
						}
					},
					{
						"source": {
							"table": "industry"
						},
						"destination": {
							"table": "industry"
						}
					},
					{
						"source": {
							"table": "lead"
						},
						"destination": {
							"table": "lead"
						}
					},
					{
						"source": {
							"table": "opportunity"
						},
						"destination": {
							"table": "opportunity"
						}
					},
					{
						"source": {
							"table": "SubDomain"
						},
						"destination": {
							"table": "SubDomain"
						}
					}
				]
			}
		},
		"folder": {
			"name": "CRM"
		},
		"annotations": []
	}
}